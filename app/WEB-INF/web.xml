<web-app
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://java.sun.com/xml/ns/javaee"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
        version="3.0">
    <context-param>
        <param-name>mariadb-host</param-name>
        <param-value>MARIA_HOST</param-value>
    </context-param>

    <context-param>
        <param-name>mariadb-user</param-name>
        <param-value>MARIA_USER</param-value>
    </context-param>

    <context-param>
        <param-name>mariadb-pwd</param-name>
        <param-value>MARIA_PWD</param-value>
    </context-param>

    <context-param>
        <param-name>mariadb-database</param-name>
        <param-value>MARIA_DATABASE</param-value>
    </context-param>

    <context-param>
        <param-name>redis-pwd</param-name>
        <param-value>REDIS_PWD</param-value>
    </context-param>

    <listener>
        <listener-class>hoot.front.Listener.MyContextListener</listener-class>
    </listener>
</web-app>
